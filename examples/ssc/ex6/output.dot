digraph dfa {
    rankdir = LR;
    __start [style = invis, shape = point];
    __start -> "{0}" [ label = "start" ];
    node [shape = doublecircle]; "{1,3}" "{1}"
    node [shape = circle];
    "{0}" -> "{1,3}" [ label = "c" ];
    "{1,3}" -> "{4}" [ label = "c" ];
    "{4}" -> "{2}" [ label = "c" ];
    "{2}" -> "{1}" [ label = "c" ];
    "{1}" -> "{4}" [ label = "c" ];
}
