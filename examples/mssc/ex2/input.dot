digraph enfa {
    rankdir = LR;
    __start [style = invis, shape = point];
    __start -> "0" [ label = "start" ];
    node [shape = doublecircle]; "0"
    node [shape = circle];
    "1" -> "0" [ label = "e" ];
    "0" -> "1" [ label = "w" ];
    "2" -> "1" [ label = "e" ];
    "1" -> "2" [ label = "&epsilon;" ];
    "2" -> "2" [ label = "w" ];
    "0" -> "2" [ label = "&epsilon;" ];
}
