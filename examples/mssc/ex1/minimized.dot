digraph dfa {
    rankdir = LR;
    __start [style = invis, shape = point];
    __start -> "0" [ label = "start" ];
    node [shape = doublecircle]; "1" "2" "3"
    node [shape = circle];
    "0" -> "0" [ label = "a" ];
    "0" -> "1" [ label = "m" ];
    "1" -> "2" [ label = "a" ];
    "1" -> "3" [ label = "m" ];
    "2" -> "2" [ label = "a" ];
    "2" -> "1" [ label = "m" ];
    "3" -> "0" [ label = "a" ];
    "3" -> "3" [ label = "m" ];
}
