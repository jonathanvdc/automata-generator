digraph enfa {
    rankdir = LR;
    __start [style = invis, shape = point];
    __start -> "0" [ label = "start" ];
    node [shape = doublecircle]; "1" "2"
    node [shape = circle];
    "1" -> "0" [ label = "a" ];
    "2" -> "2" [ label = "a" ];
    "0" -> "2" [ label = "m" ];
    "0" -> "0" [ label = "a" ];
    "1" -> "1" [ label = "m" ];
    "2" -> "1" [ label = "&epsilon;" ];
}
