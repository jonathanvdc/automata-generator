digraph dfa {
    rankdir = LR;
    __start [style = invis, shape = point];
    __start -> "{0}" [ label = "start" ];
    node [shape = doublecircle]; "{1}"
    node [shape = circle];
    "{0}" -> "{1}" [ label = "x" ];
    "{0}" -> "{0,4}" [ label = "w" ];
    "{1}" -> "{3}" [ label = "x" ];
    "{1}" -> "{3}" [ label = "w" ];
    "{3}" -> "{2,3}" [ label = "x" ];
    "{3}" -> "{0,4}" [ label = "w" ];
    "{2,3}" -> "{2,3}" [ label = "x" ];
    "{2,3}" -> "{0,4}" [ label = "w" ];
    "{0,4}" -> "{1}" [ label = "x" ];
    "{0,4}" -> "{0,4}" [ label = "w" ];
}
