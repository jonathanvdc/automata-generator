digraph dfa {
    rankdir = LR;
    node [shape = circle];
    __start [style = invis, shape = point];
    __start -> "0" [ label = "start" ];
    node [shape = doublecircle]; "1" "2"
    node [shape = circle];
    "0" -> "2" [ label = "+" ];
    "0" -> "0" [ label = "-" ];
    "2" -> "1" [ label = "+" ];
    "2" -> "0" [ label = "-" ];
    "1" -> "0" [ label = "+" ];
    "1" -> "0" [ label = "-" ];
}
