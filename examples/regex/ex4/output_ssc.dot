digraph dfa {
    rankdir = LR;
    node [shape = circle];
    __start [style = invis, shape = point];
    __start -> "{0}" [ label = "start" ];
    node [shape = doublecircle]; "{1}"
    node [shape = circle];
    "{0}" -> "{2,3}" [ label = "x" ];
    "{0}" -> "{}" [ label = "y" ];
    "{2,3}" -> "{}" [ label = "x" ];
    "{2,3}" -> "{1}" [ label = "y" ];
    "{1}" -> "{}" [ label = "x" ];
    "{1}" -> "{}" [ label = "y" ];
    "{}" -> "{}" [ label = "x" ];
    "{}" -> "{}" [ label = "y" ];
}
