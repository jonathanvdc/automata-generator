digraph dfa {
    rankdir = LR;
    __start [style = invis, shape = point];
    __start -> "0" [ label = "start" ];
    node [shape = doublecircle]; "2" "1"
    node [shape = circle];
    "0" -> "2" [ label = "c" ];
    "0" -> "1" [ label = "w" ];
    "1" -> "0" [ label = "c" ];
    "1" -> "0" [ label = "w" ];
    "2" -> "0" [ label = "c" ];
    "2" -> "0" [ label = "w" ];
}
